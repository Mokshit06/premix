{
  "name": "premix",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "node src/build && node src/watcher",
    "dev": "yarn clear && concurrently \"yarn watch\" \"yarn dev:server\"",
    "watch": "nodemon --quiet --watch app/pages -e tsx,jsx,js --exec cross-env WATCH=true node src/build.js",
    "build": "yarn build:prod",
    "build:prod": "yarn clear && cross-env NODE_ENV=production node src/build && rm -rf .premix/build/*.css .premix/build/*.webp",
    "build:dev": "yarn clear && cross-env NODE_ENV=development node src/build",
    "clear": "rm -rf .premix/*",
    "start": "cross-env PORT=3003 NODE_ENV=production node --enable-source-maps .premix/build/server",
    "type-check": "tsc",
    "prerender": "cross-env PRERENDER=true yarn build && rm -rf out && node .premix/build/prerender"
  },
  "dependencies": {
    "@next/react-dev-overlay": "^10.2.3",
    "@prisma/client": "^2.21.2",
    "@types/node-fetch": "^2.5.10",
    "@vanilla-extract/css": "^0.4.1",
    "@vanilla-extract/esbuild-plugin": "^0.2.1",
    "esbuild": "0.11.17",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "history": "^5.0.0",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-router-dom": "6.0.0-beta.0",
    "sharp": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.14",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.11",
    "@types/morgan": "^1.9.2",
    "@types/nodemon": "^1.19.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/sharp": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-preval": "^5.0.0",
    "babel-plugin-remove-unused-import": "^2.1.1",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.21.2",
    "typescript": "^4.2.4"
  }
}
