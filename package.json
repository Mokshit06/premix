{
  "name": "react-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "nodemon --watch build --exec node build/server",
    "dev": "concurrently \"yarn watch\" \"yarn dev:server\"",
    "watch": "WATCH=true node scripts/build",
    "build": "yarn build:prod",
    "build:prod": "yarn clear && NODE_ENV=production node scripts/build",
    "build:dev": "yarn clear && NODE_ENV=development node scripts/build",
    "clear": "rm -rf build public/build",
    "start": "NODE_ENV=production node build/server",
    "prerender": "node build/prerender",
    "type-check": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "@types/node-fetch": "^2.5.10",
    "esbuild": "^0.11.11",
    "eval": "^0.1.6",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react-router-dom": "^5.2.0",
    "regexparam": "^1.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7",
    "prisma": "^2.21.2"
  }
}
