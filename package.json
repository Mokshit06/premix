{
  "name": "premix",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "node scripts/build && node src/watcher",
    "dev": "yarn clear && concurrently \"yarn watch\" \"yarn dev:server\"",
    "watch": "WATCH=true node scripts/build",
    "build": "yarn build:prod",
    "build:prod": "yarn clear && NODE_ENV=production node scripts/build && rm -rf build/*.css",
    "build:dev": "yarn clear && NODE_ENV=development node scripts/build",
    "clear": "rm -rf build public/build",
    "start": "NODE_ENV=production node build/server",
    "type-check": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "@types/node-fetch": "^2.5.10",
    "@types/react-router-dom": "^5.1.7",
    "esbuild": "^0.11.11",
    "eval": "^0.1.6",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-router-dom": "^5.2.0",
    "regexparam": "^1.3.0",
    "sharp": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.14",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/nodemon": "^1.19.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/sharp": "^0.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-remove-unused-import": "^2.1.1",
    "chokidar": "^3.5.1",
    "compression": "^1.7.4",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.21.2"
  }
