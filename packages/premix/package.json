{
  "name": "@premix/core",
  "version": "1.0.0",
  "main": "dist/premix/index.js",
  "license": "MIT",
  "bin": {
    "premix": "./dist/bin/premix.js"
  },
  "scripts": {
    "dev:server": "node src/build && node src/watcher",
    "dev": "yarn clear && concurrently \"yarn watch\" \"yarn dev:server\"",
    "watch": "WATCH=true node src/build",
    "build": "yarn build:prod",
    "build:prod": "yarn clear && NODE_ENV=production node src/build && rm -rf build/*.css build/*.webp",
    "build:dev": "yarn clear && NODE_ENV=development node src/build",
    "clear": "rm -rf build public/build",
    "start": "NODE_ENV=production node --enable-source-maps build/server",
    "type-check": "tsc",
    "prerender": "PRERENDER=true yarn build && rm -rf out && node build/prerender"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "@types/react-router-dom": "^5.1.7",
    "commander": "^7.2.0",
    "esbuild": "^0.11.11",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "node-fetch": "^2.6.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-router-dom": "6.0.0-beta.0",
    "sharp": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.14",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "@types/nodemon": "^1.19.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/sharp": "^0.28.0",
    "babel-plugin-preval": "^5.0.0",
    "babel-plugin-remove-unused-import": "^2.1.1",
    "chalk": "^4.1.1",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "nodemon": "^2.0.7"
  }
}
